{
  "name": "bosket-root",
  "version": "0.0.1",
  "description": "A collection of tree views.",
  "main": "index.js",
  "author": "Julien Elbaz",
  "license": "MIT",
  "repository": "https://github.com/elbywan/bosket",
  "scripts": {
    "start": "npm run clean && npm run build && npm run build-docs",
    "prebuild": "eslint ./ && flow",
    "build": "npm run build-react && npm run build-angular",
    "postbuild": "copy README.md ./build && flow stop",
    "build-react": "babel src -d build -s && flow-copy-source -v src build",
    "build-angular": "ngc -p src/angular/tsconfig.json && cd src && copy ./**/*.d.ts ../build",
    "build-docs": "npm install build --no-save && webpack --progress --colors --config docs/webpack.config.js -p",
    "dev-server": "webpack-dev-server --config docs/webpack.config.dev.js",
    "clean": "rimraf build/{angular,core,react,tools} && rimraf **/.DS_Store"
  },
  "files": [
    "build",
    "index.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@ngtools/webpack": "^1.4.2",
    "@types/node": "^8.0.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "copy": "^0.3.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.1.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.48.0",
    "flow-copy-source": "^1.1.0",
    "html-webpack-plugin": "^2.29.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nesting": "^4.0.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-transition-group": "^1.2.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.2.0",
    "typescript": "^2.4.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0",
    "zone.js": "^0.8.12"
  }
}
