{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": false,
        "allowJs": false,
        "lib": [ "es2015", "dom" ],
        "baseUrl": "../..",
        "paths": {
            "self/*": ["./docs/*"]
        }
    },
    "include": [
        "./**/*.ts"
    ]
}
